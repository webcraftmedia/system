<tr class="sai_log_error ${class_row}" onClick="document.location = '#!log(error);error.${ID}';">
    <td>${time}</td>                            
    <td>${class}</td>
    <td style="word-break: break-all;">${message}</td>
    <td style="word-break: break-all;">${file}</td>
    <td>${line}</td>
    <td>${ip}</td>
    <td style="word-break: break-all;">${server_name}:${server_port}${request_uri}</td>
    <td>${username}</td>
    <td>${querytime}</td>
</tr>